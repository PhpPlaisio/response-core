{
  "name": "setbased/abc-response-core",
  "description": "SetBased ABC-Framework: Core Response",
  "keywords": [
    "SetBased ABC-Framework",
    "Core",
    "Response"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-mbstring": "*",
    "ext-json": "*",
    "setbased/abc-html": "^2.2",
    "setbased/abc-response": "^1.0.0",
    "setbased/abc-url": "^2.0.0",
    "setbased/exception": "^2.0.0",
    "setbased/helper-cast": "^1.2.0",
    "willdurand/jsonp-callback-validator": "^1.1.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\Response\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Abc\\Response\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
