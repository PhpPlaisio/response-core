{
  "name": "plaisio/response-core",
  "description": "PhpPlaisio: Core Response",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Response"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-json": "*",
    "ext-mbstring": "*",
    "plaisio/cookie": "^2.2.0",
    "plaisio/helper-html": "^4.2.0",
    "plaisio/helper-ob": "^1.3.0",
    "plaisio/helper-url": "^1.3.0",
    "plaisio/kernel": "^3.3.0",
    "plaisio/response": "^2.2.0",
    "setbased/exception": "^2.6.0",
    "setbased/helper-cast": "^3.1.0",
    "willdurand/jsonp-callback-validator": "^2.0.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "plaisio/console-kernel": "^1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Response\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Response\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
