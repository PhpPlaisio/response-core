{
  "name": "plaisio/response-core",
  "description": "PhpPlaisio: Core Response",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Response"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "plaisio/html": "^3.0.0",
    "plaisio/response": "^2.0.0",
    "plaisio/url": "^3.0.0",
    "setbased/exception": "^2.0.0",
    "setbased/helper-cast": "^2.0.0",
    "willdurand/jsonp-callback-validator": "^1.1.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Response\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Response\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
